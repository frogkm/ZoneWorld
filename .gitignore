Alien/*.pidb.meta
Alien/*.pdb.meta
Alien/*.mdb.meta

# Unity3D generated file on crash reports
Alien/sysinfo.txt

# Builds
Alien/*.apk
Alien/*.aab
Alien/*.unitypackage
Alien/*.app

# Crashlytics generated file
Alien/crashlytics-build.properties

# Packed Addressables
Alien/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

